{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
            "functions": {
                "api/index.js": {
                    "maxDuration": 60
                }
            }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ]
  }
  